
function highLow () {
  var randomNum = Math.floor((Math.random() * 100) + 1);
  console.log(randomNum);

  do {
    var int = parseInt(prompt("Guess a number"));
    if (int < randomNum) {
      alert("Your guess is too low!");
    }
    if (int > randomNum) {
      alert("Your guess is too high!");
    }
  } while (int !== randomNum);

  alert("You win again!");
}
